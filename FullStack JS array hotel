var findVacantRooms = function(array){
	var vacancy = [];
	for(var i =0; i< array.length;i++){
	 if(array[i]==='vacant'){
	 	vacancy.push(i);
	 }
	 else{
		vacancy= vacancy;
	}
	
	}
	return vacancy;
};


var findCheapestVacantRoom = function(array){
	var vacancy = [];
	var price = [];
	for(var i =0; i< array.length;i++){
	 if(typeof array[i] === 'number'){
	 	price.push(array[i]);
	 }
	 else{
		price = price;
	 }
	}
	if(price.length>0)
	{
	var minprice = [Math.min.apply(null, price)];
	minprice.unshift(price.indexOf(Math.min.apply(null, price)));
	return minprice;
	}
	else{
		return price;
	}
};
